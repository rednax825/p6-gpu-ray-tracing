void hitTriangle(vec3 vert0, vec3 vert1, vec3 vert2, Ray ray, inout Hit nearestHit, int mat)
{
	vec3 edge1 = vert1 - vert0;
	vec3 edge2 = vert2 - vert0;
	vec3 h = cross(ray.dir, edge2);
	float a = dot(edge1, h);
	// check if ray is parallel to triangle, backfacing triangles are also culled here
	if(a < 0.000001)
	{
		return;
	}
	
	float f = 1.0 / a;
	vec3 s = ray.origin - vert0;
	float u = f * dot(s, h);
	if(u < 0.0 || u > 1.0)
	{
		return;
	}
	
	vec3 q = cross(s, edge1);
	float v = f * dot(ray.dir, q);
	if(v < 0.0 || v + u > 1.0)
	{
		return;
	}
	
	// calc distance to intersection
	float t = f * dot(edge2, q);

	// if t is positive it is a valid hit, if t is nearest record it
	if(t > 0.000001 && t < nearestHit.dist)
	{
		nearestHit.dist = t;
		nearestHit.pos = ray.origin + t * ray.dir;
		nearestHit.normal = normalize(cross(edge1, edge2));
		nearestHit.mat = mat;
	}
}

void hitSphere(vec3 center, float radius, Ray ray, inout Hit nearestHit, int mat)
{
	vec3 originToCenter = ray.origin - center;
	
	// quadratic formula to check intersection
	float a = dot(ray.dir, ray.dir);
	float b = 2.0 * dot(originToCenter, ray.dir);
	float c = dot(originToCenter, originToCenter) - radius * radius;
	float descriminant = b * b - 4 * a * c;
	
	// if descriminant is less than 0, ray missed
	if(descriminant < 0)
	{
		return;
	}
	else
	{
		// ray hit, calc distance from origin to point of intersection
		float t = (-b - sqrt(descriminant)) / (2.0 * a);

		if(t > 0 && t < nearestHit.dist)
		{
			nearestHit.dist = t;
			nearestHit.pos = ray.origin + t * ray.dir;
			nearestHit.normal = normalize(nearestHit.pos - center);
			nearestHit.mat = mat;
		}
	}
}

void hitGround(Ray ray, inout Hit nearestHit, int mat)
{
	float t = -ray.origin.y / ray.dir.y;
	if(t > 0 && t < nearestHit.dist)
	{
		nearestHit.dist = t;
		nearestHit.pos = ray.origin + t * ray.dir;
		nearestHit.normal = vec3(0., 1., 0.);
		nearestHit.mat = mat;
	}
}

Hit traceScene(Ray ray)
{
	Hit hit = Hit(vec3(0., 0., 0.), 1./0., vec3(0., 0., 0.), 0);

	// trace the model, iterating all triangles
	for(int i = 0; i < numFaces; i++)
	{
		hitTriangle(verts[faces[i].x - 1], verts[faces[i].y - 1], verts[faces[i].z - 1], ray, hit, 4);
	}

	// trace some spheres for flavor
	hitSphere(vec3(-3., 0.7, -4.), 2.0, ray, hit, 0);
	hitSphere(vec3(7., 6., -12.), 3.0, ray, hit, 1);
	hitSphere(vec3(4., 2., 3.), 1.0, ray, hit, 2);
	hitSphere(vec3(12., 12., -7.), 4.0, ray, hit, 3);

	// trace the ground plane
	hitGround(ray, hit, 5);

	return hit;
}

vec3 shade(inout Ray ray, Hit hit)
{
	if(hit.dist < 10000.)
	{
		vec3 specular;
		vec3 albedo;
		switch(hit.mat)
		{
			case 0:
				specular = uSphere1Specular.xyz;
				albedo = uSphere1Albedo.xyz;
				break;
			case 1:
				specular = uSphere2Specular.xyz;
				albedo = uSphere2Albedo.xyz;
				break;
			case 2:
				specular = uSphere3Specular.xyz;
				albedo = uSphere3Albedo.xyz;
				break;
			case 3:
				specular = uSphere4Specular.xyz;
				albedo = uSphere4Albedo.xyz;
				break;
			case 4:
				specular = uCatSpecular.xyz;
				albedo = uCatAlbedo.xyz;
				break;
			case 5:
				specular = uGroundSpecular.xyz;
				albedo = uGroundAlbedo.xyz;
				break;
		}
		// move hit my a little so that floating point inaccuracy doesnt cause it to collide the same surface again
		ray.origin = hit.pos + hit.normal * 0.001;
		ray.dir = reflect(ray.dir, hit.normal);
		ray.energy = ray.energy * specular;

		// trace shadow test ray
		Ray shadowRay = Ray(hit.pos + hit.normal * 0.001, lightSourceVector, vec3(0., 0., 0.));
		Hit shadowHit = traceScene(shadowRay);
		if(shadowHit.dist < 10000.)
		{
			return vec3(0., 0., 0.);
		}

		// return diffuse
		return clamp(dot(hit.normal, lightSourceVector), 0., 1.) * uLightIntensity.rgb * albedo;
	}
	else
	{
		// ray hit skybox, use its remaining energy to sample
		ray.energy = vec3(0., 0., 0.);
		return texture(uCubeMap, ray.dir).rgb;
	}
}

void main()
{
	int maxBounces = int(uMaxBounces);
	vec3 color = vec3(0., 0., 0.);

	// calculate inital ray direction and origin
	Ray ray = Ray(near.xyz/near.w, normalize((far.xyz/far.w) - (near.xyz/near.w)), vec3(1., 1., 1.));

	// bounce ray through scene up until max bounces
	for(int b = 0; b < maxBounces; b++)
	{
		Hit hit = traceScene(ray);
		vec3 energy = ray.energy;
		color = color + (energy * shade(ray, hit));

		// end ray early if it is out of energy
		if(ray.energy.x < 0.01 && ray.energy.y < 0.01 && ray.energy.z < 0.01)
		{
			break;
		}
	}
	
	gl_FragColor = vec4(color, 1.);
}